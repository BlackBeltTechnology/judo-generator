[%
  import "../../../operations/_importAll.eol";
%]

CREATE TABLE [%= table.sqlName %] /*[%= table.shortName %]*/ (
	[% for (field in table.fields) { %]
		[%= field.sqlName %] [%= field.toFieldDefinition() %] [% if (field.isPrimary()) { %] NOT NULL PRIMARY KEY[% } %] /*[%= field.shortName%]*/ [% if (hasMore) { out.print(","); } %] 
	[% } %]
);[% out.println("\n"); %]
