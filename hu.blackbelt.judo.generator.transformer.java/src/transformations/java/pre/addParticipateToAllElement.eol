operation addParticipateToAllElement () : Any {
	for (c in PSM!JClass.all) { 
		var seq = new Sequence;
		c.package.collectParticipatesTo(seq);
		c.participates.addAll(seq);
	}
	
	for (c in PSM!JEnumeration.all) { 
		var seq = new Sequence;
		c.package.collectParticipatesTo(seq);
		c.participates.addAll(seq);
	}
}

operation PSM!JPackage collectParticipatesTo (all : Sequence) : Any {
	all.addAll(self.participates);
	if (self.parent.isDefined()) {
		self.parent.collectParticipatesTo(all);
	}
}