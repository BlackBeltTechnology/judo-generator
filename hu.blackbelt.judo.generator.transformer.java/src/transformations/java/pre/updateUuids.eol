operation updateUuids() {

	var modelNames = PSM!JModel.all.collect(m | m.name.replace(" ", "_")).asSet();

	for (element in PSM!JElement.all.select(e | not e.isTypeOf(PSM!JModel))) {
		for (modelName in modelNames) {
			if (modelName.isSubstringOf(element.uuid)) {
				element.uuid = element.uuid.replace(modelName, modelName + "." + "hu" + "." + "blackbelt");
			}
		}
	}
}