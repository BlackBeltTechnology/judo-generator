import "../../../operations/_importAll.eol";
import "element.etl";

@abstract
rule JClass 
	transform s : PSM!JClass
	to t : JAVA!JvmClass
	extends JElement {
		t.package ::= s.package;
}

@abstract
rule JClassInterface 
	transform s : PSM!JClass
	to t : JAVA!JvmInterface
	extends JElement {
		t.package ::= s.package;
}

