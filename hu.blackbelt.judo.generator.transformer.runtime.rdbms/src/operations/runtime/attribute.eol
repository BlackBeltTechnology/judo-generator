import "modelElement.eol";

@cached
operation RUNTIME!EAttribute isID() : Boolean {
	return self.annotatedAsTrue("id");
}

@cached
operation RUNTIME!EAttribute getClass() : RUNTIME!EClass {
	return RUNTIME!EClass.all.selectOne(c | c.eAllAttributes.includes(self));
}

@cached
operation RUNTIME!EAttribute fqName() : String {
	return self.getClass().fqName() + "#" + self.name;
}
