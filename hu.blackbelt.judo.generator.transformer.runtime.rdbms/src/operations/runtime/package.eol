@cached
operation RUNTIME!EPackage getPackage() : RUNTIME!EPackage {
	return RUNTIME!EPackage.all.selectOne(p | p.eSubPackages.contains(self));
}

@cached
operation RUNTIME!EPackage fqName() : String {
	var name = "";
	if (self.getPackage().isDefined()) {
		name = self.getPackage().fqName() + ".";
	}
	return name + self.name;
}
