    <eLiterals name="CREATE"/>
    <eLiterals name="DELETE" value="1" literal="DELETE"/>
    <eLiterals name="MODIFY" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="RdbmsFieldOperationType">
    <eLiterals name="CREATE"/>
    <eLiterals name="DELETE" value="1"/>
    <eLiterals name="MODIFY" value="2"/>
  </eClassifiers>
