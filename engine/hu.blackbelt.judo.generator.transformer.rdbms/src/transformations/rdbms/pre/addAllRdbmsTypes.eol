operation addAllRdbmsFieldTypes () : Any {
	for (s: RDBMSTYPES!RdbmsFieldType in RDBMSTYPES!RdbmsFieldType.all) {
		var t : RDBMS!RdbmsFieldType = new RDBMS!RdbmsFieldType;
		for (attr : RDBMS!EAttribute in t.eClass.getEAllAttributes()) {
			t.eSet(attr, s.eGet(attr));
		}		
	}	
}

