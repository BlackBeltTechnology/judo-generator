	[% } %]
	"fields" : [
	[% if(not self.parameters.isEmpty())  { %]
		[%for (parameter in self.parameters) { %]
			[%parameter.generateJson();%] [% if (hasMore) { out.println(","); } %]
		[%}%]
	[% } %]
	],
	"id" : "[%=self.id%]",
	"notBulk" : [%=self.notBulk%],
	[% if(self.result.isDefined())  { %]
	"result" : [%=self.result.generateJson()%],
	[% } else {%]
