[% operation UI!UIBaseComponentType generateJson() { %]
{
	[% if (self.upper = -1) { %]
	"upper" : "*",
	[% } else { %]
	"upper" : "[%=self.upper%]",
	[% } %]
	"referenced" : null,
	
	[% if(self.lower > 0)  { %]
	"mandatory" : true,
	[% } else {%]
	"mandatory" : false,
	[% } %]
	
	"private" : [%=self.private%],
	"single" : false,
	"intervals" : [
	[% if(not self.intervals.isEmpty())  { %]	
		[%for (interval in self.intervals) { %]
			[%interval.generateJson();%] [% if (hasMore) { out.println(","); } %]
		[%}%]
	[% } %]
	],
	
	"id" : "[%=self.id%]",
	"readonly" : [%=self.readonly%],
	"inherited" : [%=self.inherited%],
	"type" : "[%=self.type%]",
	"derived" : [%=self.derived%],
	"navigable" : false,
	"component" : "[%=self.type%]",
	[% if (self.regexp.isDefined()) {%]
	"regexp" : "[%=self.regexp.asString().replace("\\\\","\\\\\\\\")%]",
	[% } else {%]
	"regexp" : "[%=self.regexp%]",
	[% } %]
	[% if(self.placeholder.isDefined())  { %]
	"placeholder" : "[%=self.placeholder%]",
	[% } else {%]
	"placeholder" : "",
	[% } %]
	"association" : false,
	"lower" : "[%=self.lower%]",
	"name" : "[%=self.name%]",
	[% if (self.interval.isDefined()) {%]
	"interval" : "[%=self.interval%]",
	[% } else { %]
	"interval" : "",
	[% } %]
	"list" : false,
	[% if(self.value.isDefined())  { %]
	"value" : "[%=self.value%]",
	[% } else {%]
	"value" : "",
	[%}%]
	"disableOnCreate" : false,
	"representation" : [%=self.representation%],
	[% if(self.decimals.isDefined())  { %]
	"decimals" : "[%=self.decimals%]"
	[% } else {%]
	"decimals" : null
	[% } %]
}
[% } %]

[% operation UI!UIReferenceComponentType generateJson() { %]
{
	[% if (self.upper = -1) { %]
	"upper" : "*",
	[% } else { %]
	"upper" : "[%=self.upper%]",
	[% } %]
	"referenced" : "[%=self.referenced.uuid%]",
	[% if(self.lower > 0)  { %]
	"mandatory" : true,
	[% } else {%]
	"mandatory" : false,
	[% } %]
	
	"private" : [%=self.private%],
	[% if (self.upper = 1) {%]
	"single" : true,
	[% } else {%]
	"single" : false,
	[% } %]
	"intervals" : [
	[% if(not self.intervals.isEmpty())  { %]	
		[%for (interval in self.intervals) { %]
			[%interval.generateJson();%] [% if (hasMore) { out.println(","); } %]
		[%}%]
	[% } %]
	],
	
	"id" : "[%=self.id%]",
	"readonly" : [%=self.readonly%],
	[% if(self.opposite.isDefined()) {%]
	"other" : "[%=self.opposite.id %]",
	[% } else { %]
	"other" : null,
	[% } %]
	"inherited" : [%=self.inherited%],
	"type" : "[%=self.type%]",
	"derived" : [%=self.derived%],
	"navigable" : [%=self.navigable%],
	"component" : "[%=self.type%]",
	"kind" : "[%=self.kind%]",
	"regexp" : "",
	"placeholder" : "",
	"association" : true,
	"lower" : "[%=self.lower%]",
	"name" : "[%=self.name%]",
	[% if (self.interval.isDefined()) {%]
	"interval" : "[%=self.interval%]",
	[% } else { %]
	"interval" : "",
	[% } %]
	
	[% if (self.upper > 1 or self.upper = -1) {%]
	"list" : true,
	[% } else {%]
	"list" : false,
	[% } %]
	[% if(self.value.isDefined())  { %]
	"value" : "[%=self.value%]",
	[% } else {%]
	"value" : "",
	[%}%]
	"disableOnCreate" : false,
	"representation" : false,
	"decimals" : null
}
[% } %]


[% operation UI!UIParameterComponentType generateJson() { %]
{
	[% if (self.upper = -1) { %]
	"upper" : "*",
	[% } else { %]
	"upper" : "[%=self.upper%]",
	[% } %]
	[% if (self.referenced.isDefined()) { %] 
	"referenced" : "[%=self.referenced.id%]",
	[% } else {%]
	"referenced" : null,
	[% } %]
	[% if(self.lower > 0)  { %]
	"mandatory" : true,
	[% } else {%]
	"mandatory" : false,
	[% } %]
	
	"private" : false,
	[% if (self.referenced.isDefined() and self.upper = 1) {%]
	"single" : true,
	[% } else {%]
	"single" : false,
	[% } %]
	"intervals" : [
	[% if(self.intervals.isDefined() and not self.intervals.isEmpty())  { %]	
		[%for (interval in self.intervals) { %]
			[%interval.generateJson();%] [% if (hasMore) { out.println(","); } %]
		[%}%]
	[% } %]
	],
	
	"id" : "[%=self.id%]",
	"readonly" : false,
	"inherited" : false,
	"type" : "[%=self.type%]",
	"derived" : false,
	"navigable" : false,
	"component" : "[%=self.type%]",
	"regexp" : "",
	"placeholder" : "",
	"association" : false,
	"lower" : "[%=self.lower%]",
	"name" : "[%=self.name%]",
	[% if (self.interval.isDefined()) {%]
	"interval" : "[%=self.interval%]",
	[% } else { %]
	"interval" : "",
	[% } %]
	[% if (self.referenced.isDefined() and (self.upper > 1 or self.upper = -1)) {%]
	"list" : true,
	[% } else {%]
	"list" : false,
	[% } %]
	[% if(self.value.isDefined())  { %]
	"value" : "[%=self.value%]",
	[% } else {%]
	"value" : "",
	[%}%]
	"disableOnCreate" : false,
	"representation" : false,
	"decimals" : null
}
[% } %]

